    <message xml:lang="tr">Mauna Disk Kalıbı Yazıcı için yetkilendirme gerekiyor</message>
    <message xml:lang="zh_CN">运行 莫纳 图像编写器 需要身份验证</message>            
    <defaults>
      <allow_any>auth_admin</allow_any>
      <allow_inactive>auth_admin</allow_inactive>
